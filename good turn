#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);  // Read number of test cases

    for (int i = 0; i < T; i++) {
        int X, Y;
        scanf("%d %d", &X, &Y);  // Read dice rolls
        if (X + Y > 6)
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}
